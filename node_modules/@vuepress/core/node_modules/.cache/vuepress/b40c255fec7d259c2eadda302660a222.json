erWidth < 720 ? true : false;\n    })\n  },\n  methods: {\n    toggle () {\n      if (this.isMQMobile) {\n        this.open = !this.open\n      }\n    },\n\n    isLastItemOfArray (item, array) {\n      return last(array) === item\n    }\n  },\n\n  watch: {\n    $route () {\n      this.open = false\n    }\n  }\n}\n"]}]}