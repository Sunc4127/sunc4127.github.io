    "vuepress-plugin-one-click-copy": "^1.0.2",
    "vuepress-plugin-thirdparty-search": "^1.0.2",
    "vuepress-plugin-zooming": "^1.1.7",
    "vuepress-theme-vdoing": "^1.12.8",
    "yamljs": "^0.3.0"
  },
  "dependencies": {
    "markdown-it": "^12.2.0",
    "markdown-it-anchor": "^8.4.1",
